{
  "name": "supplement_smarter_backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "scrape:nzprotein": "tsx src/scrapers/protein/nzProtein_Protein.ts",
    "scrape:xplosiv": "tsx src/scrapers/protein/xplosiv_Protein.ts",
    "scrape:sprintfit": "tsx src/scrapers/protein/sprintFit_Protein.ts",
    "scrape:nowhey": "tsx src/scrapers/protein/noWhey_Protein.ts",
    "scrape:chemistwarehouse": "tsx src/scrapers/common/scrape.ts chemistwarehouse",
    "scrape:sportsfuel": "tsx src/scrapers/protein/scrape.ts sportsfuel",
    "scrape:all": "tsx src/scrapers/protein/scrapeAllProtein.ts",
    "scrape:test": "cross-env SCRAPE_TESTING=1 MAX_MS=60000 tsx src/scrapers/protein/scrapeAllProtein.ts",
    "scrape:creatine:nzprotein": "tsx src/scrapers/creatine/nzProtein_Creatine.ts",
    "scrape:creatine:xplosiv": "tsx src/scrapers/creatine/xplosiv_Creatine.ts",
    "scrape:creatine:sprintfit": "tsx src/scrapers/creatine/sprintFit_Creatine.ts",
    "scrape:creatine:nowhey": "tsx src/scrapers/creatine/noWhey_Creatine.ts",
    "scrape:creatine:cw": "tsx src/scrapers/creatine/CW_Creatine.ts",
    "scrape:creatine:all": "tsx src/scrapers/creatine/scrapeAllCreatine.ts",
    "scrape:creatine:test": "cross-env SCRAPE_TESTING=1 MAX_MS=60000 tsx src/scrapers/creatine/scrapeAllCreatine.ts",
    "db:clean": "npx tsx SQL/runSQL.ts SQL/00_clean.sql",
    "db:apply:schema": "npx tsx SQL/runSQL.ts SQL/01_schema.sql",
    "db:apply:norm": "npx tsx SQL/runSQL.ts SQL/02_normalise.sql",
    "db:build:protein": "npx tsx SQL/runSQL.ts SQL/03_filter_protein.sql && npx tsx SQL/runSQL.ts SQL/04_build_protein.sql",
    "db:build:creatine": "npx tsx SQL/runSQL.ts SQL/03_filter_creatine.sql && npx tsx SQL/runSQL.ts SQL/04_build_creatine.sql",
    "db:build": "npm run db:build:protein && npm run db:build:creatine",
    "db:apply:debug": "npx tsx SQL/runSQL.ts SQL/99_debug_views.sql",
    "db:psql": "SQL\\psql.bat -d supplements",
    "norm:rerun": "npm run db:apply:norm && npm run db:build",
    "db:apply:all": "npm run db:clean && npm run db:apply:schema && npm run db:apply:norm && npm run db:build",
    "db:reset-and-scrape": "npm run db:clean && npm run db:apply:schema && npm run scrape:all && npm run scrape:creatine:all && npm run db:apply:norm && npm run db:build",
    "db:rescrape": "npm run scrape:all && npm run scrape:creatine:all && npm run db:apply:norm && npm run db:build",
    "dev": "npx tsx src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "cheerio": "^1.1.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "^0.19.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  }
}
